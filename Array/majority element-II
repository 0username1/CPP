//Given an integer array of size n, find all elements that appear more than ⌊ n/3 ⌋ times.

#include<bits/stdc++.h>
using namespace std;

void majorityElement(int* arr , int n)
{
    unordered_map<int , int> mp;
    for(int i=0;i<n;i++)
    {
        mp[arr[i]]++;
    }
    cout<<"All elements that appear more than n/3 times are : ";
    for(auto x : mp)
    {
        if(x.second > (n / 3))
        {
            cout<<x.first<<" ";
        }
    }
}

int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    majorityElement(arr , n);
    return 0;
}
